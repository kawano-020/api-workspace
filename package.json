{
  "name": "workspace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:prd": "NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier '**/*.{ts,js,vue}' --ignore-path .gitignore -c",
    "format:eslint": "npm run lint -- --fix",
    "format:prettier": "npm run lint:prettier -- -w",
    "format": "npm run format:prettier && npm run format:eslint",
    "bundle-openapi": "swagger-cli bundle -o ./docs/api/openapi.yaml -t yaml docs/api/index.yaml",
    "generate-": "npm run bundle-openapi && redoc-cli bundle ./docs/api/openapi.yaml --output ./static/redoc.html"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/markdownit": "^2.0.0",
    "camelcase-keys": "^7.0.1",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
    "redoc-cli": "^0.13.7",
    "swagger-cli": "^4.0.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.5.1",
    "sass": "^1.44.0",
    "sass-loader": "^10.1.1"
  }
}
